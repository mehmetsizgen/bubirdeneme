generator client {
  provider = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Villas {
  id Int @id @default(autoincrement())
  json Json? @db.Json
  add_date String? @db.VarChar(255)
  add_member Int @default(0)
  member Json @db.Json
  owner Json @db.Json
  regions Json @db.Json
  regions_up Json @db.Json
  categorys Json @db.Json
  distances Json @db.Json
  prices Json @db.Json
  features Json @db.Json
  comments Json @db.Json
  availability Json @db.Json
  reservation Json @db.Json
  agency Json @db.Json
  order Int? @default(0)
  company_slug String? @db.VarChar(255)
  status Int @default(1)
  show_status Int @default(0)
  @@map("villas")
}
